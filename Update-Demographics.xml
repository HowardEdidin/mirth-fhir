          <script>logger.info(&quot;Initiating Update demographics...&quot;);
responseMap.put(&apos;result&apos;,msg);

var fhir = new fhirObj().init(&quot;cerner&quot;,&quot;R4&quot;,msg.token);
var PatientId =  msg.patient_id;

var Payload = &quot;[\n&quot; + 
				&quot;    {\n&quot; + 
				&quot;    \&quot;path\&quot;: \&quot;/name/0/id\&quot;,\n&quot; + 
				&quot;    \&quot;op\&quot;: \&quot;test\&quot;,\n&quot; + 
				&quot;    \&quot;value\&quot;: \&quot;CI-&quot; + PatientId + &quot;-0\&quot;\n&quot; + 
				&quot;    },\n&quot; + 
				&quot;    {\n&quot; + 
				&quot;        \&quot;path\&quot;: \&quot;/name/0/given\&quot;,\n&quot; + 
				&quot;        \&quot;op\&quot;: \&quot;replace\&quot;,\n&quot; + 
				&quot;        \&quot;value\&quot;: [\n&quot; + 
				&quot;            \&quot;Ganesh\&quot;,\n&quot; + 
				&quot;            \&quot;Jayaraman\&quot;\n&quot; + 
				&quot;        ]\n&quot; + 
				&quot;    },\n&quot; + 
				&quot;    {\n&quot; + 
				&quot;        \&quot;path\&quot;: \&quot;/name/0/family\&quot;,\n&quot; + 
				&quot;        \&quot;op\&quot;: \&quot;replace\&quot;,\n&quot; + 
				&quot;        \&quot;value\&quot;: \&quot;SHANKAR\&quot;\n&quot; + 
				&quot;    }\n&quot; + 
				&quot;]&quot;;

responseMap.put(&apos;updatePatient&apos;,fhir.updatePatient(PatientId,Payload,true));
responseMap.put(&apos;patientDemographics&apos;,fhir.getPatientDemographics(PatientId));</script>
